{
  "name": "lum_rest",
  "version": "1.29.0",
  "main": "./src/server.ts",
  "engines": {
    "node": "12.18.3",
    "npm": "6.14.6"
  },
  "apidoc": {
    "title": "Documentation of Lum API",
    "url": "https://lum-rest.herokuapp.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Devinno-company/lum-rest.git"
  },
  "author": "Devinno-company <company.devinno@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node --expose-gc --max_old_space_size=512 -r ts-node/register ./src/server.ts",
    "knex:migrate": "knex --knexfile knexfile.ts migrate:latest --env production",
    "knex:migrate:rollback": "knex --knexfile knexfile.ts migrate:rollback --env production",
    "dev": "tsnd --transpile-only --ignore-watch node_modules --respawn src/server.ts"
  },
  "dependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.4",
    "@types/request": "^2.48.5",
    "aws-sdk": "^2.766.0",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.6",
    "mercadopago": "^1.5.0",
    "multer": "^1.4.2",
    "pg": "^8.3.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "ts-node-dev": "^1.0.0"
  }
}
